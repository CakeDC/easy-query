{
  "name": "itosho/easy-query",
  "description": "CakePHP behavior plugin for easily some complicated queries.",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "behavior",
    "plugin",
    "upsert",
    "bulk upsert",
    "bulk insert",
    "insert select",
    "insert once"
  ],
  "homepage": "https://github.com/itosho/easy-query",
  "license": "MIT",
  "authors": [
    {
      "name": "itosho",
      "role": "Author",
      "homepage": "https://github.com/itosho"
    }
  ],
  "support": {
    "issues": "https://github.com/itosho/easy-query/issues",
    "source": "https://github.com/itosho/easy-query"
  },
  "require": {
    "php": ">=7.1.0",
    "cakephp/orm": "^3.6"
  },
  "require-dev": {
    "cakephp/cakephp": "^3.6",
    "phpunit/phpunit": "^5.7.14|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Itosho\\EasyQuery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Itosho\\EasyQuery\\Test\\": "tests"
    }
  }
}
